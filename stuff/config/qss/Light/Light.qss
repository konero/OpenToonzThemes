* {
  padding: 0;
  margin: 0;
  outline: 0;
  border: 0;
  border-style: none;
  border-image: none;
}
QToolBar * {
  padding: 0;
  margin: 0;
}
QWidget {
  background-color: #e8e8e8;
  color: #000000;
}
QWidget:disabled {
  color: rgba(0, 0, 0, 0.4);
}
/* -----------------------------------------------------------------------------
   MainWindow & Floating Panels
----------------------------------------------------------------------------- */
QFrame {
  border: 0;
  margin: 0;
  padding: 0;
}
QToolTip,
#helpToolTip {
  background-color: #fff;
  border: 1 solid #808080;
  color: #000;
  padding: 1 1;
}
#DockSeparator,
QMainWindow::separator,
QSplitter::handle {
  background-color: #7c7c7c;
  height: 4;
  width: 4;
}
#TDockPlaceholder {
  background-color: #1aff00;
}
TPanel {
  background-color: #7c7c7c;
}
/* -----------------------------------------------------------------------------
   Topbar
----------------------------------------------------------------------------- */
#TopBar {
  background-color: #e8e8e8;
  border: 0;
  /* Accent border against docked panels, requires margin-bottom in
     #TopBarTabContainer to be seen! */
  border-bottom: 1 solid #bbbbbb;
  margin: 0;
}
#TopBar #EditToolLockButton {
  background-color: #e8e8e8;
  spacing: 0;
}
#TopBar #EditToolLockButton::indicator {
  background: none;
  border: none;
  padding-left: 0;
  padding-right: 0;
}
#TopBarTabContainer {
  background-color: #e8e8e8;
  margin-bottom: 1;
}
#StackedMenuBar {
  border: 0;
  margin: 0;
  padding: 0;
}
QMenuBar {
  background-color: #e8e8e8;
  border: 0;
}
QMenuBar::item {
  background-color: #e8e8e8;
  border-left: 1 solid #e8e8e8;
  padding: 4 5;
}
QMenuBar::item:selected {
  background-color: rgba(255, 255, 255, 0.15);
  color: #fff;
}
QMenuBar::item:pressed {
  background-color: #ffc0a0;
  color: #000000;
}
/* -----------------------------------------------------------------------------
   Rooms Workspaces
----------------------------------------------------------------------------- */
#TopBarTab {
  margin: 0;
  padding: 0;
  border: 0;
  background-color: transparent;
}
#TopBarTab::tab {
  background-color: #d2d2d2;
  padding: 3 8 4 8;
  margin: 0 -1 0 0;
  border-color: #bbbbbb;
  border-radius: 0;
  border-bottom-width: 0;
}
#TopBarTab::tab:hover {
  background-color: #b9b9b9;
}
#TopBarTab::tab:selected {
  background-color: #ffc0a0;
  border-color: #bbbbbb;
  color: #000000;
}
#TopBarTab::tab:selected:hover {
  background-color: #ff9e6d;
}
#TopBarTab::tab:first {
  border-left-width: 1;
  border-top-left-radius: 0;
}
#TopBarTab::tab:last {
  margin-right: 0;
  border-top-right-radius: 0;
}
#TopBarTab::tab:only-one {
  border-left-width: 1;
}
#TopBarTab QToolButton {
  border-left: 2 solid #bbbbbb;
  border-top: 1 solid #bbbbbb;
  border-right: 1 solid #bbbbbb;
  border-bottom: 0;
  margin-left: -1;
  margin-top: 0;
}
/* -----------------------------------------------------------------------------
   Context Menu
----------------------------------------------------------------------------- */
QMenu {
  background-color: #e8e8e8;
  border: 1 solid #bbbbbb;
  color: #000000;
  padding: 2 0;
}
QMenu::item {
  border: 0;
  padding: 3 28 3 14;
}
QMenu::item:selected {
  background-color: #ffc0a0;
  color: #000000;
}
QMenu::item:checked {
  color: #9fdaff;
  background-color: #e8e8e8;
}
QMenu::item:checked:selected {
  background-color: #ffc0a0;
  color: #000000;
}
QMenu::item:disabled {
  background: none;
  color: rgba(0, 0, 0, 0.4);
}
QMenu::item:disabled:selected {
  color: rgba(0, 0, 0, 0.4);
}
QMenu::separator {
  border-top: 1 solid #ffffff;
  height: 0;
  margin: 2 0;
}
QMenu::icon {
  border-radius: 2;
  position: absolute;
  left: 7;
  padding: 0;
  margin: 0;
}
QMenu::icon:checked {
  background-color: #ffc0a0;
  border: 1 solid #ffc0a0;
}
QMenu::indicator {
  margin-left: 8;
}
QMenu::indicator:non-exclusive {
  background-color: white;
  border: 1 solid white;
  border-radius: 2;
  padding: 1;
  width: 9;
  height: 9;
  border-color: white;
}
QMenu::indicator:non-exclusive:hover {
  background-color: white;
  border-color: #fff;
}
QMenu::indicator:non-exclusive:checked {
  background-color: #ffc0a0;
  border-color: #ffc0a0;
  image: url('../Default/imgs/black/checkmark.svg');
}
QMenu::indicator:non-exclusive:checked:hover {
  background-color: #ff9e6d;
  border-color: #fff;
}
QMenu::indicator:non-exclusive:checked:disabled {
  background-color: #dedede;
  border-color: #dedede;
  image: url('../Default/imgs/black/checkmark_disabled.svg');
}
QMenu::indicator:non-exclusive:indeterminate {
  background-color: #ffc0a0;
  border-color: #ffc0a0;
  image: url('../Default/imgs/black/checkpartially.svg');
}
QMenu::indicator:non-exclusive:indeterminate:hover {
  background-color: white;
  border-color: #fff;
}
QMenu::indicator:non-exclusive:indeterminate:disabled {
  background-color: #f1d8cb;
  border-color: #f1d8cb;
  image: url('../Default/imgs/black/checkpartially_disabled.svg');
}
QMenu::indicator:non-exclusive:disabled {
  background-color: #f1f1f1;
  border-color: #f1f1f1;
}
QMenu::indicator:non-exclusive:selected {
  border-color: #fff;
}
QMenu::indicator:exclusive {
  width: 13;
  height: 13;
  background-color: transparent;
  border: 0;
  image-position: center center;
}
QMenu::indicator:exclusive:unchecked {
  image: url('../Default/imgs/black/radiobutton_unchecked.svg');
}
QMenu::indicator:exclusive:unchecked:hover {
  image: url('../Default/imgs/black/radiobutton_unchecked_hover.svg');
}
QMenu::indicator:exclusive:checked {
  image: url('../Default/imgs/black/radiobutton_checked.svg');
}
QMenu::indicator:exclusive:checked:disabled {
  background-color: transparent;
  image: url('../Default/imgs/black/radiobutton_checked_disabled.svg');
}
QMenu::indicator:exclusive:disabled {
  image: url('../Default/imgs/black/radiobutton_unchecked_disabled.svg');
}
/* -----------------------------------------------------------------------------
   Titlebars
----------------------------------------------------------------------------- */
TPanelTitleBar {
  background-color: #d2d2d2;
  border-bottom: 1 solid #bbbbbb;
  border-top-left-radius: 0;
  border-top-right-radius: 0;
  height: 18;
  min-height: 18;
  qproperty-TitleColor: rgba(0, 0, 0, 0.6);
  qproperty-ActiveTitleColor: black;
  qproperty-OverColor: hsl(0, 57%, 58%);
}
TPanelTitleBar:hover {
  background-color: #d2d2d2;
}
TPanelTitleBarButton {
  qproperty-PressedColor: #ffc0a0;
  qproperty-FreezeColor: hsl(0, 57%, 58%);
  qproperty-PreviewColor: hsl(50, 45%, 45%);
  qproperty-OverColor: #b9b9b9;
}
/* -----------------------------------------------------------------------------
   Scrollbars
----------------------------------------------------------------------------- */
QAbstractScrollArea::corner {
  background-color: #c7c7c7;
}
QScrollBar {
  background-color: #c7c7c7;
  border: 0;
}
QScrollBar:horizontal {
  height: 15;
  margin: 0;
}
QScrollBar:vertical {
  margin: 0;
  width: 15;
}
QScrollBar::handle {
  border: 0 solid #e8e8e8;
  border-radius: 4;
}
QScrollBar::handle:horizontal:hover,
QScrollBar::handle:vertical:hover {
  background-color: #cfcfcf;
}
QScrollBar::handle:horizontal:pressed,
QScrollBar::handle:vertical:pressed {
  background-color: #b5b5b5;
}
QScrollBar::handle:horizontal {
  background-color: #e8e8e8;
  margin: 3 16;
  min-width: 20;
}
QScrollBar::handle:vertical {
  background-color: #e8e8e8;
  margin: 16 3;
  min-height: 20;
}
QScrollBar::add-line {
  subcontrol-origin: margin;
  border: 0;
}
QScrollBar::add-line:horizontal {
  subcontrol-position: right;
  background-color: #c7c7c7;
  margin: 0;
  width: 16;
}
QScrollBar::add-line:vertical {
  subcontrol-position: bottom;
  background-color: #c7c7c7;
  margin: 0;
  height: 16;
}
QScrollBar::sub-line {
  border: 0;
  subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal {
  subcontrol-position: left;
  background-color: #c7c7c7;
  margin: 0;
  width: 16;
}
QScrollBar::sub-line:vertical {
  subcontrol-position: top;
  background-color: #c7c7c7;
  margin: 0;
  height: 16;
}
QScrollBar::up-arrow:vertical {
  image: url('../Default/imgs/black/scroll-up.svg');
  image-position: center center;
}
QScrollBar::up-arrow:vertical:hover {
  image: url('../Default/imgs/black/scroll-up-hover.svg');
}
QScrollBar::up-arrow:vertical:pressed {
  margin: 1 0 0 0;
  image: url('../Default/imgs/black/scroll-up-hover.svg');
}
QScrollBar::down-arrow:vertical {
  image: url('../Default/imgs/black/scroll-down.svg');
  image-position: center center;
}
QScrollBar::down-arrow:vertical:hover {
  image: url('../Default/imgs/black/scroll-down-hover.svg');
}
QScrollBar::down-arrow:vertical:pressed {
  margin: 1 0 0 0;
  image: url('../Default/imgs/black/scroll-down-hover.svg');
}
QScrollBar::left-arrow:horizontal {
  image: url('../Default/imgs/black/scroll-left.svg');
  image-position: center center;
}
QScrollBar::left-arrow:horizontal:hover {
  image: url('../Default/imgs/black/scroll-left-hover.svg');
}
QScrollBar::left-arrow:horizontal:pressed {
  margin: 1 0 0 0;
  image: url('../Default/imgs/black/scroll-left-hover.svg');
}
QScrollBar::right-arrow:horizontal {
  image: url('../Default/imgs/black/scroll-right.svg');
  image-position: center center;
}
QScrollBar::right-arrow:horizontal:hover {
  image: url('../Default/imgs/black/scroll-right-hover.svg');
}
QScrollBar::right-arrow:horizontal:pressed {
  margin: 1 0 0 0;
  image: url('../Default/imgs/black/scroll-right.svg');
}
QScrollBar::sub-page:horizontal,
QScrollBar::add-page:horizontal,
QScrollBar::sub-page:vertical,
QScrollBar::add-page:vertical {
  background: none;
}
/* -----------------------------------------------------------------------------
   Progress Bar
----------------------------------------------------------------------------- */
QProgressBar {
  text-align: center;
  background-color: #bdbdbd;
  border: 1 solid #bbbbbb;
  border-radius: 3;
  padding: 0;
}
QProgressBar::chunk {
  margin: -1;
  background-color: #14a037;
  border: 1 solid #bbbbbb;
  border-radius: 2;
}
/* -----------------------------------------------------------------------------
   Buttons
----------------------------------------------------------------------------- */
QPushButton {
  background-color: #d2d2d2;
  border-color: #bdbdbd;
  border-width: 1;
  border-style: solid;
  border-radius: 2;
  color: #000000;
  margin: 0;
  padding: 2 15;
}
QPushButton:hover {
  background-color: #b9b9b9;
  color: black;
  border-color: #b9b9b9;
}
QPushButton:pressed {
  background-color: #bdbdbd;
  border-color: #bdbdbd;
  color: #000000;
}
QPushButton:checked {
  background-color: #b2b2b2;
  border-color: #bdbdbd;
  color: #9fdaff;
}
QPushButton:checked:hover {
  background-color: #989898;
  border-color: #bdbdbd;
}
QPushButton:checked:hover:pressed {
  background-color: #cbcbcb;
}
QPushButton:disabled {
  background-color: #dfdfdf;
  border-color: #d7d7d7;
  color: rgba(0, 0, 0, 0.4);
}
#PushButton_NoPadding {
  padding: 3;
}
#CustomPanelButton {
  background-color: #d2d2d2;
  border-color: #bdbdbd;
  border-width: 1;
  border-style: solid;
  border-radius: 2;
  color: #000000;
  margin: 0;
  padding: 2 15;
  padding: 0;
}
#CustomPanelButton:hover {
  background-color: #b9b9b9;
  color: black;
  border-color: #b9b9b9;
}
#CustomPanelButton:pressed {
  background-color: #bdbdbd;
  border-color: #bdbdbd;
  color: #000000;
}
#CustomPanelButton:checked {
  background-color: #b2b2b2;
  border-color: #bdbdbd;
  color: #9fdaff;
}
#CustomPanelButton:checked:hover {
  background-color: #989898;
  border-color: #bdbdbd;
}
#CustomPanelButton:checked:hover:pressed {
  background-color: #cbcbcb;
}
#CustomPanelButton:disabled {
  background-color: #dfdfdf;
  border-color: #d7d7d7;
  color: rgba(0, 0, 0, 0.4);
}
/* -----------------------------------------------------------------------------
   ComboBox
----------------------------------------------------------------------------- */
QComboBox {
  background-color: #d2d2d2;
  border-color: #bdbdbd;
  border-width: 1;
  border-style: solid;
  border-radius: 2;
  color: #000000;
  margin: 0;
  padding: 1 0 1 4;
}
QComboBox:hover {
  background-color: #b9b9b9;
  color: black;
  border-color: #b9b9b9;
}
QComboBox:checked {
  border-color: #ffc0a0;
}
QComboBox:pressed {
  background-color: #d2d2d2;
  border-color: #bdbdbd;
  color: #000;
}
QComboBox:pressed:focus {
  background-color: #d2d2d2;
  border-color: #ffc0a0;
}
QComboBox:disabled {
  background-color: #dfdfdf;
  border-color: #d7d7d7;
  color: rgba(0, 0, 0, 0.4);
}
QComboBox:focus {
  background-color: #d2d2d2;
  border-color: #ffc0a0;
}
QComboBox:focus:hover {
  background-color: #b9b9b9;
}
QComboBox:editable {
  color: black;
  background-color: white;
  border-color: #bbbbbb;
  selection-background-color: rgba(255, 192, 160, 0.8);
  selection-color: #000000;
  padding: 1 0 1 3;
}
QComboBox:editable:hover {
  background-color: white;
}
QComboBox:editable:checked {
  border-color: #ffc0a0;
}
QComboBox:editable:focus {
  border-color: #ffc0a0;
}
QComboBox::drop-down {
  border: 0 solid transparent;
  image: url('../Default/imgs/black/combo_downarrow.svg');
  image-position: center center;
  width: 16;
}
QComboBox::drop-down:disabled {
  image: url('../Default/imgs/black/combo_downarrow_disabled.svg');
}
QComboBox::drop-down:disabled:editable {
  background-color: #dfdfdf;
}
QComboBox::drop-down:editable {
  background-color: #d2d2d2;
  border-left: 1 solid #bbbbbb;
  border-top-right-radius: 0;
  border-bottom-right-radius: 0;
}
QComboBox::drop-down:editable:hover {
  background-color: #b9b9b9;
}
QComboBox QAbstractItemView {
  background-color: #e8e8e8;
  selection-background-color: #ffc0a0;
  selection-color: #000000;
  border: 1 solid #bbbbbb;
}
/* -----------------------------------------------------------------------------
   Text Fields
----------------------------------------------------------------------------- */
QPlainTextEdit,
QLineEdit,
QTextEdit[readOnly="false"] {
  background-color: white;
  border: 1 solid #bdbdbd;
  border-radius: 2;
  color: black;
  selection-background-color: rgba(255, 192, 160, 0.8);
  selection-color: #000000;
  padding: 1 0 1 1;
}
QPlainTextEdit:hover,
QLineEdit:hover,
QTextEdit[readOnly="false"]:hover {
  background-color: white;
  color: black;
  border-color: #bdbdbd;
}
QPlainTextEdit:focus,
QLineEdit:focus,
QTextEdit[readOnly="false"]:focus {
  background-color: white;
  border-color: #ffc0a0;
  color: #fff;
}
QPlainTextEdit:disabled,
QLineEdit:disabled,
QTextEdit[readOnly="false"]:disabled {
  background-color: #f1f1f1;
  border-color: #d7d7d7;
  color: rgba(0, 0, 0, 0.4);
}
QTextEdit[readOnly="true"] {
  background-color: #e8e8e8;
  color: #000000;
  border: 0;
}
QTextEdit[readOnly="true"]:focus,
QTextEdit[readOnly="true"]:disabled {
  background-color: #e8e8e8;
  color: #000000;
  border: 0;
}
/* -----------------------------------------------------------------------------
   Options
----------------------------------------------------------------------------- */
QCheckBox {
  background-color: transparent;
  color: #000000;
}
QCheckBox:hover {
  color: #fff;
}
QCheckBox:disabled {
  color: rgba(0, 0, 0, 0.4);
}
QCheckBox::indicator {
  background-color: white;
  border: 1 solid white;
  border-radius: 2;
  padding: 1;
  width: 9;
  height: 9;
}
QCheckBox::indicator:hover {
  background-color: white;
  border-color: #fff;
}
QCheckBox::indicator:checked {
  background-color: #ffc0a0;
  border-color: #ffc0a0;
  image: url('../Default/imgs/black/checkmark.svg');
}
QCheckBox::indicator:checked:hover {
  background-color: #ff9e6d;
  border-color: #fff;
}
QCheckBox::indicator:checked:disabled {
  background-color: #dedede;
  border-color: #dedede;
  image: url('../Default/imgs/black/checkmark_disabled.svg');
}
QCheckBox::indicator:indeterminate {
  background-color: #ffc0a0;
  border-color: #ffc0a0;
  image: url('../Default/imgs/black/checkpartially.svg');
}
QCheckBox::indicator:indeterminate:hover {
  background-color: white;
  border-color: #fff;
}
QCheckBox::indicator:indeterminate:disabled {
  background-color: #f1d8cb;
  border-color: #f1d8cb;
  image: url('../Default/imgs/black/checkpartially_disabled.svg');
}
QCheckBox::indicator:disabled {
  background-color: #f1f1f1;
  border-color: #f1f1f1;
}
QRadioButton {
  color: #000000;
  padding: 0;
  margin: 0;
}
QRadioButton:hover {
  color: #fff;
}
QRadioButton:checked {
  color: #000000;
}
QRadioButton:disabled {
  color: rgba(0, 0, 0, 0.4);
}
QRadioButton::indicator {
  width: 13;
  height: 13;
  background-color: transparent;
  border: 0;
  image-position: center center;
}
QRadioButton::indicator:unchecked {
  image: url('../Default/imgs/black/radiobutton_unchecked.svg');
}
QRadioButton::indicator:unchecked:hover {
  image: url('../Default/imgs/black/radiobutton_unchecked_hover.svg');
}
QRadioButton::indicator:checked {
  image: url('../Default/imgs/black/radiobutton_checked.svg');
}
QRadioButton::indicator:checked:disabled {
  background-color: transparent;
  image: url('../Default/imgs/black/radiobutton_checked_disabled.svg');
}
QRadioButton::indicator:disabled {
  image: url('../Default/imgs/black/radiobutton_unchecked_disabled.svg');
}
/* -----------------------------------------------------------------------------
   GroupBox
----------------------------------------------------------------------------- */
QGroupBox {
  background-color: transparent;
  border-width: 1;
  border-style: solid;
  border-radius: 2;
  border-color: #bbbbbb;
  margin: 6 0 0 0;
  padding: 5 5;
}
QGroupBox:unchecked {
  border-color: #d6d6d6;
}
QGroupBox::title {
  subcontrol-origin: margin;
  color: #000000;
  left: 15;
  margin: -2 0 0 0;
  padding: 0 3;
}
QGroupBox::title:hover {
  color: #fff;
}
QGroupBox::title:disabled {
  color: rgba(0, 0, 0, 0.4);
}
QGroupBox::indicator {
  background-color: white;
  border: 1 solid white;
  border-radius: 2;
  padding: 1;
  width: 9;
  height: 9;
  subcontrol-origin: margin;
  margin-top: 2;
}
QGroupBox::indicator:hover {
  background-color: white;
  border-color: #fff;
}
QGroupBox::indicator:checked {
  background-color: #ffc0a0;
  border-color: #ffc0a0;
  image: url('../Default/imgs/black/checkmark.svg');
}
QGroupBox::indicator:checked:hover {
  background-color: #ff9e6d;
  border-color: #fff;
}
QGroupBox::indicator:checked:disabled {
  background-color: #dedede;
  border-color: #dedede;
  image: url('../Default/imgs/black/checkmark_disabled.svg');
}
QGroupBox::indicator:indeterminate {
  background-color: #ffc0a0;
  border-color: #ffc0a0;
  image: url('../Default/imgs/black/checkpartially.svg');
}
QGroupBox::indicator:indeterminate:hover {
  background-color: white;
  border-color: #fff;
}
QGroupBox::indicator:indeterminate:disabled {
  background-color: #f1d8cb;
  border-color: #f1d8cb;
  image: url('../Default/imgs/black/checkpartially_disabled.svg');
}
QGroupBox::indicator:disabled {
  background-color: #f1f1f1;
  border-color: #f1f1f1;
}
QGroupBox:disabled {
  color: rgba(0, 0, 0, 0.4);
}
/* -----------------------------------------------------------------------------
   Sliders
----------------------------------------------------------------------------- */
QSlider {
  background-color: transparent;
}
QSlider::sub-page {
  background-color: #a7a7a7;
  border: 1 solid #a7a7a7;
  border-radius: 2;
}
QSlider::sub-page:disabled {
  background-color: #cecece;
  border-color: #cecece;
}
QSlider::sub-page:horizontal {
  margin: 8 0 8 0;
}
QSlider::sub-page:vertical {
  margin: 0 8 0 8;
}
QSlider::add-page {
  background-color: #a7a7a7;
  border: 1 solid #a7a7a7;
  border-radius: 2;
}
QSlider::add-page:disabled {
  background-color: #cecece;
  border-color: #cecece;
}
QSlider::add-page:horizontal {
  margin: 8 0 8 0;
}
QSlider::add-page:vertical {
  margin: 0 8 0 8;
}
QSlider::groove {
  background-color: transparent;
  margin: 0;
}
QSlider::groove:horizontal {
  height: 20;
  min-height: 20;
}
QSlider::groove:vertical {
  width: 20;
  min-width: 20;
}
QSlider::handle:horizontal {
  width: 10;
  image: url('../Default/imgs/black/slider-handle-rectangle.svg');
}
QSlider::handle:horizontal:disabled {
  image: url('../Default/imgs/black/slider-handle-rectangle_disabled.svg');
}
